<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ink.dao.BackgroundDao">

    <!-- Result maps describe the mapping between the columns returned
   from a query, and the class properties.  A result map isn't
   necessary if the columns (or aliases) match to the properties
   exactly. -->
    <resultMap id="BackgroundResult" type="com.ink.entity.Background">
        <result property="id" column="id"/>
        <result property="background_type" column="background_type"/>
        <result property="background_color" column="background_color"/>
        <result property="background_img1" column="background_img1"/>
        <result property="background_img2" column="background_img2"/>
        <result property="background_img3" column="background_img3"/>
        <result property="background_img4" column="background_img4"/>
        <result property="background_img6" column="background_img6"/>
        <result property="background_img9" column="background_img9"/>
    </resultMap>
</mapper>